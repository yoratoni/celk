{
    "entries": [
        "src/wasm/src/index.ts"
    ],
    "targets": {
        "dev": {
            "outFile": "src/lib/binary/index.wasm",
            "textFile": "src/lib/binary/index.wat",
            "sourceMap": true,
            "debug": true
        },
        "prod": {
            "outFile": "src/lib/binary/index.wasm",
            "textFile": "src/lib/binary/index.wat",
            "sourceMap": true,
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": true,
            "noAssert": false
        }
    },
    "options": {
        "bindings": "esm"
    }
}