{
    "entries": [
        "src/assembly/src/index.ts"
    ],
    "targets": {
        "dev": {
            "outFile": "src/assembly/build/index.wasm",
            "textFile": "src/assembly/build/index.wat",
            "sourceMap": true,
            "debug": true
        },
        "prod": {
            "outFile": "src/assembly/build/index.wasm",
            "textFile": "src/assembly/build/index.wat",
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "sourceMap": true,
            "converge": false,
            "noAssert": false
        }
    },
    "options": {
        "bindings": "esm",
        "uncheckedBehavior": "always"
    }
}